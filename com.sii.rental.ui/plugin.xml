<?xml version="1.0" encoding="UTF-8"?>
<plugin>
   <extension-point id="palette" name="palette" schema="schema/palette.exsd"/>

   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="com.sii.rental.ui.customerSelected">
         <instanceof
               value="com.opcoach.training.rental.Customer">
         </instanceof>
      </definition>
   </extension>
   <extension
         point="com.opcoach.e4.preferences.e4PreferencePages">
      <page
            name="Colors"
            class="com.sii.rental.ui.prefs.RentalColorPage"
            id="com.sii.rental.ui.pageColor">
      </page>
      <page
            class="com.sii.rental.ui.prefs.RentalPalettePreferences"
            id="com.sii.rental.ui.palette"
            name="palette">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.sii.rental.ui.prefs.RentalPrefsInitializer">
      </initializer>
   </extension>
   <extension
         id="com.sii.rental.ui.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="fragment.e4xmi">
      </fragment>
   </extension>
   <extension
         point="com.sii.rental.ui.palette">
      <palette
            id="com.sii.rental.ui.defaultPalette"
            name="defaultPalette"
            palette="com.sii.rental.ui.defaultPalette">
      </palette>
      <palette
            id="com.sii.rental.ui.mochePalette"
            name="mochePalette"
            palette="com.sii.rental.ui.mochePalette">
      </palette>
   </extension>

</plugin>
